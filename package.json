  "dependencies": {
    "@better-auth/expo": "^1.3.27",
    "better-auth": "^1.3.27",
    "expo-constants": "^18.0.10",
    "expo-linking": "^8.0.8",
    "expo-web-browser": "^15.0.8",
    "expo": "~54.0.20",
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "turbo": "^2.5.4"
  },
  "packageManager": "npm@11.0.0",
  "version": "1.0.0"
